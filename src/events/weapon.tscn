script/source = "extends AnimationPlayer

# class member variables go here, for example:
# var a = 2
# var b = \"textvar\"

export var delay = 0;

func _ready():
	set_process(true)

var started = false
var timebuffer = 0.0

func _process(delta):
	if started: return
	
	timebuffer += delta
	if timebuffer >= delay:
		started = true
		play(\"text-appear\")
"

